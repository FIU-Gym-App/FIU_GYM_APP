<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/txt_GymSchedule_MainActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FIU GYM SCHEDULE HERE" />

    <Button
        android:id="@+id/btn_CheckIn_MainActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"

        android:backgroundTint="#36AA3B"
        android:text="CheckIn" />

    <Button
        android:id="@+id/btn_Bar_MainActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn_Logout_MainActivity"
        android:layout_below="@+id/btn_CheckIn_MainActivity"
        android:layout_centerHorizontal="true"

        android:backgroundTint="#E69722"
        android:text="Bar" />

</LinearLayout>